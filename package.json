        "@kinshipjs/json": "^0.0.6-development",
        "@lucia-auth/adapter-test": "^4.1.0",
        "@types/node": "^20.3.1",
        "crypto": "^1.0.1",
        "dotenv": "^16.0.3",
        "tsup": "^7.2.0",
        "typescript": "^4.9.4"
